{
  "name": "mozu-fe",
  "packageManager": "yarn@4.12.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn workspace @mozu/admin start\" \"yarn workspace @mozu/student start:ip\"",
    "admin": "yarn workspace @mozu/admin",
    "student": "yarn workspace @mozu/student",
    "ui": "yarn workspace @mozu/ui",
    "util": "yarn workspace @mozu/util-config",
    "dev": "concurrently \"yarn workspace @mozu/admin dev\" \"yarn workspace @mozu/student dev\"",
    "dev:ip": "concurrently \"yarn workspace @mozu/admin dev\" \"yarn workspace @mozu/student start:ip\"",
    "build": "concurrently \"yarn workspace @mozu/admin build\" \"yarn workspace @mozu/student build\"",
    "build:dev": "concurrently \"yarn workspace @mozu/admin build:dev\" \"yarn workspace @mozu/student build:dev\"",
    "build:prod": "concurrently \"yarn workspace @mozu/admin build:prod\" \"yarn workspace @mozu/student build:prod\"",
    "prepare": "husky install",
    "format": "biome format",
    "commit": "cz",
    "new-branch": "sh ./scripts/new-branch.sh"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.3.0",
    "react-spinners": "^0.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^22.10.5",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "corepack": "^0.34.0",
    "cz-customizable": "^7.4.0",
    "husky": "^8.0.0",
    "jira-prepare-commit-msg": "^1.7.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "supports-color": "^10.1.0",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.1",
    "@types/hoist-non-react-statics": "^3.3.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "[$J] $M",
    "allowEmptyCommitMessage": false,
    "ignoredBranchesPattern": "^(main|develop)$"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
